<%= form_for @content do |f| %>
  <%= render "showreel_content", :content => @content %>

  <%= f.label :media_link %>
  <%= f.text_field :media_link %>

  <%= f.label :header %>
  <%= f.text_field :header %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit "Save" %>
<% end %>
