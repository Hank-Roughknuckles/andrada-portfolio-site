<div class="overlay"></div> <!-- used when a tile is clicked -->

<div class="gridster">
  <ul>
  <% for content in @contents  %>
    <li class="grid_tile" 
    id="tile_<%= content.id %>"
    data-row="<%= content.grid_row %>" 
    data-col="<%= content.grid_column %>" 
    data-sizex="<%= content.grid_sizex %>" 
    data-sizey="<%= content.grid_sizey %>" 
    style="<%= get_tile_image_css Work, content.id %>"></li>
    <%= render "work_content", :content => content %>
  <% end %>
  </ul>
</div>
